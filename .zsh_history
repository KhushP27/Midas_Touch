JBlock::JBlock(int level)\
    : Block{'J', level} {\
\
    cells.clear();\
    cells.reserve(4);\
\
    int rowBottom = 3;\
    int rowTop = 2;\
    int colLeft = 3;\
\
    // Shape:\
    //  J\
    // JJJ\
    cells.emplace_back(rowBottom, colLeft);         // bottom left\
    cells.emplace_back(rowBottom, colLeft + 1);     // bottom middle\
    cells.emplace_back(rowBottom, colLeft + 2);     // bottom right\
    cells.emplace_back(rowTop,    colLeft);         // top left\
}\
