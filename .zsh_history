import position;\
import block;\
import iblock;\
import jblock;\
import lblock;\
import oblock;\
import sblock;\
import zblock;\
import tblock;\
import <vector>;\
import <iostream>;\
import <cassert>;\
\
bool sameCells(const std::vector<Position> &a,\
               const std::vector<Position> &b) {\
    if (a.size() != b.size()) return false;\
\
    bool used[4] = {false, false, false, false};\
\
    for (std::size_t i = 0; i < a.size(); ++i) {\
        bool found = false;\
        for (std::size_t j = 0; j < b.size(); ++j) {\
            if (used[j]) continue;\
            if (a[i].getRow() == b[j].getRow() &&\
                a[i].getCol() == b[j].getCol()) {\
                used[j] = true;\
                found = true;\
                break;\
            }\
        }\
        if (!found) return false;\
    }\
    return true;\
}\
\
// Expected spawn cells for each block type, based on your current constructors\
\
std::vector<Position> expectedI() {\
    std::vector<Position> v;\
    int row = 3;\
    int startCol = 3;\
    v.emplace_back(row, startCol);\
    v.emplace_back(row, startCol + 1);\
    v.emplace_back(row, startCol + 2);\
    v.emplace_back(row, startCol + 3);\
    return v;\
}\
\
std::vector<Position> expectedJ() {\
    std::vector<Position> v;\
    int rowBottom = 3;\
    int rowTop = 2;\
    int colLeft = 3;\
    v.emplace_back(rowBottom, colLeft);\
    v.emplace_back(rowBottom, colLeft + 1);\
    v.emplace_back(rowBottom, colLeft + 2);\
    v.emplace_back(rowTop,    colLeft);\
    return v;\
}\
\
std::vector<Position> expectedL() {\
    std::vector<Position> v;\
    int rowBottom = 3;\
    int rowTop = 2;\
    int colLeft = 3;\
    v.emplace_back(rowBottom, colLeft);\
    v.emplace_back(rowBottom, colLeft + 1);\
    v.emplace_back(rowBottom, colLeft + 2);\
    v.emplace_back(rowTop,    colLeft + 2);\
    return v;\
}\
\
std::vector<Position> expectedO() {\
    std::vector<Position> v;\
    int rowBottom = 3;\
    int rowTop = 2;\
    int colLeft = 4;\
    v.emplace_back(rowTop,    colLeft);\
    v.emplace_back(rowTop,    colLeft + 1);\
    v.emplace_back(rowBottom, colLeft);\
    v.emplace_back(rowBottom, colLeft + 1);\
    return v;\
}\
\
std::vector<Position> expectedS() {\
    std::vector<Position> v;\
    int rowBottom = 3;\
    int rowTop = 2;\
    int colLeft = 3;\
    // Shape:\
    //  SS\
    // SS\
    v.emplace_back(rowTop,    colLeft + 1);\
    v.emplace_back(rowTop,    colLeft + 2);\
    v.emplace_back(rowBottom, colLeft);\
    v.emplace_back(rowBottom, colLeft + 1);\
    return v;\
}\
\
std::vector<Position> expectedZ() {\
    std::vector<Position> v;\
    int rowBottom = 3;\
    int rowTop = 2;\
    int colLeft = 3;\
    // Shape:\
    // ZZ\
    //  ZZ\
    v.emplace_back(rowTop,    colLeft);\
    v.emplace_back(rowTop,    colLeft + 1);\
    v.emplace_back(rowBottom, colLeft + 1);\
    v.emplace_back(rowBottom, colLeft + 2);\
    return v;\
}\
\
std::vector<Position> expectedT() {\
    std::vector<Position> v;\
    int rowBottom = 3;\
    int rowTop = 2;\
    int colLeft = 3;\
    // Shape:\
    // TTT\
    //  T\
    v.emplace_back(rowTop,    colLeft);\
    v.emplace_back(rowTop,    colLeft + 1);\
    v.emplace_back(rowTop,    colLeft + 2);\
    v.emplace_back(rowBottom, colLeft + 1);\
    return v;\
}\
\
// Generic test for one block type: spawn + 4x rotation\
template <typename BlockType>\
void testBlock(const char *name,\
               const std::vector<Position> &expectedSpawn) {\
    BlockType b{0};  // spawn level doesn't matter for geometry\
\
    const std::vector<Position> &cells = b.getCells();\
    assert(cells.size() == 4);\
    assert(sameCells(cells, expectedSpawn));\
\
    // Check 4 rotations bring it back to original shape\
    std::vector<Position> before = b.getCells();\
    b.rotateCW();\
    b.rotateCW();\
    b.rotateCW();\
    b.rotateCW();\
    const std::vector<Position> &after = b.getCells();\
    assert(sameCells(before, after));\
\
    std::cout << name << " spawn + rotation test passed\n";\
}\
\
// Extra: for IBlock, also confirm the 90 degree orientation we expect\
void testIExtra() {\
    IBlock b{0};\
    std::vector<Position> start = b.getCells();\
\
    b.rotateCW();\
    const std::vector<Position> &after = b.getCells();\
\
    // After one CW rotation, I should be vertical, same col,\
    // rows 0..3 (given our current rotation logic).\
    std::vector<Position> expected90;\
    int col = start[0].getCol(); // should be 3\
    expected90.emplace_back(0, col);\
    expected90.emplace_back(1, col);\
    expected90.emplace_back(2, col);\
    expected90.emplace_back(3, col);\
\
    assert(sameCells(after, expected90));\
    std::cout << "IBlock 90 degree rotation test passed\n";\
}\
\
int main() {\
    testBlock<IBlock>("IBlock", expectedI());\
    testIExtra();\
\
    testBlock<JBlock>("JBlock", expectedJ());\
    testBlock<LBlock>("LBlock", expectedL());\
    testBlock<OBlock>("OBlock", expectedO());\
    testBlock<SBlock>("SBlock", expectedS());\
    testBlock<ZBlock>("ZBlock", expectedZ());\
    testBlock<TBlock>("TBlock", expectedT());\
\
    std::cout << "All block spawn + rotation tests passed\n";\
}\
EOF\

