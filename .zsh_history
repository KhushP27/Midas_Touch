import <string>;\
\
export class HighScoreManager {\
    std::string filename;\
    int highScore;\
\
public:\
    explicit HighScoreManager(const std::string &filename);\
\
    int getHighScore() const;\
\
    // Returns true if high score was updated\
    bool updateIfHigher(int score);\
};\
EOF\

cat << 'EOF' > highscoremanager-impl.cc\
module highscoremanager;\
\
import <string>;\
import <fstream>;\
import <iostream>;\
\
HighScoreManager::HighScoreManager(const std::string &file)\
    : filename{file}, highScore{0} {\
\
    std::ifstream in{filename};\
    if (in) {\
        int value;\
        if (in >> value && value >= 0) {\
            highScore = value;\
        }\
    }\
}\
\
int HighScoreManager::getHighScore() const {\
    return highScore;\
}\
\
bool HighScoreManager::updateIfHigher(int score) {\
    if (score <= highScore) return false;\
\
    highScore = score;\
\
    std::ofstream out{filename};\
    if (!out) {\
        // If we cannot write, at least keep the in-memory value\
        std::cerr << "Warning: could not write high score file: "\
                  << filename << "\n";\
        return true;\
    }\
\
    out << highScore << "\n";\
    return true;\
}\
EOF\

cat << 'EOF' > test-highscore.cc\
import highscoremanager;\
import <fstream>;\
import <cassert>;\
import <iostream>;\
\
int main() {\
    const char *fname = "test_highscore.txt";\
\
    // Reset file to 0 for deterministic testing\
    {\
        std::ofstream reset{fname};\
        reset << 0 << "\n";\
    }\
\
    HighScoreManager mgr{fname};\
    assert(mgr.getHighScore() == 0);\
\
    // Update to 10\
    bool changed = mgr.updateIfHigher(10);\
    assert(changed);\
    assert(mgr.getHighScore() == 10);\
\
    // Updating with lower score should do nothing\
    changed = mgr.updateIfHigher(5);\
    assert(!changed);\
    assert(mgr.getHighScore() == 10);\
\
    // Updating with higher score\
    changed = mgr.updateIfHigher(25);\
    assert(changed);\
    assert(mgr.getHighScore() == 25);\
\
    // New manager should read 25 from file\
    HighScoreManager mgr2{fname};\
    assert(mgr2.getHighScore() == 25);\
\
    std::cout << "HighScoreManager tests passed\n";\
}\
EOF\

